<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta http-equiv="Content-Type" content="text/html"; charset=utf-8" />

        <!-- Mobile -->

        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

        <!--[if lt IE 9]>

        <script src="http://ie7-js.googlecode.com/svn/version/2.1(beta4)/IE9.js"></script>

        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>

        <![endif]-->
        <title>{% block title %}HealthCareAbroad{% endblock %}</title>

        {% javascripts
        
            'js/main/jquery.js'
			'js/main/jquery.tools.min.js'
			'js/main/jquery.cycle.all.min.js'
			'js/main/jquery.jplayer.min.js'
			'js/main/jquery.tweet.js'
			'js/main/jquery.prettyPhoto.js'
			'js/main/jquery.tipsy.js'
			'js/main/jquery.li-scroller.1.0.js'
			'js/main/jquery.fitvids.js'
			'js/main/custom.js'
        %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% block stylesheets %}
             <link href="{{ asset('css/frontend/shortcodes.css') }}" rel="stylesheet"/>
            <link href="{{ asset('css/frontend/style_splash.css') }}" rel="stylesheet"/>
            <!-- Google Fonts -->
			<link href='http://fonts.googleapis.com/css?family=Play:400,700' rel='stylesheet' type='text/css'> 
            
        {% endblock %}
    </head>
    {% block body %}
    	
    	<body> 
    
		    <div class="fixed">
		
				<div id="top" class="top_bar">		
				    <div class="inner">		        
				            <div class="news_ticker">		
				            </div><!--News Ticker-->		
				    </div> <!--End Inner-->		
				</div> <!--End Topbar-->
			
				<header id="header">	
				    <div class="top_line"></div>		
					<div class="inner">
					    <div class="container">		
					        <article class="cat_article">
					
							    <div id="article_content" class="single_article_content">
							    	<br /> <a href="index.html"><img src="{{ asset('images/frontend/logo3.png') }}" alt="#" /> </a>
									
									<div class="brdr3"></div>
								        <h1 style="font-size:14px;margin-top:8px;">HealthCareAbroad.com&#8482; is a directory that provides a World of Healthcare Options.</h1><br />
									
									<div class="box_note box clear" style="">
									   
										<h1 class="box_head" style="border-bottom-color:; background-color:#e4e4e4;color:#444;-moz-border-radius-topleft:; -moz-border-radius-topright:px; border-top-left-radius:px; border-top-right-radius:px; font-size:16px; height:8px;"></h1>
										<h2>Be among the first to know when we launch Healthcare Abroad!<br />
										<strong>Sign up below:</strong></h2><br />
										
									    <div class="contact_form">  
									    	 {% include '::notice.admin.html.twig' %}
										    <form novalidate="novalidate"  id="newsletterForm" action="{{ path('newsletter_new') }}" method="POST" {{ form_enctype(form) }} class="basic-form">
								   
								                  {{ form_row(form.email) }}
		                                			<input type="submit" name="submit" id="newsletterButton" value="Notify me" class="form-submit button black_b" />
		                                			 {{ form_rest(form) }}
                          		
								            </form>		
											
								        </div> <!--End Contact Form-->
									
									</div>					
								    <div class="clear"></div>
								</div> <!--Single Article content-->		
									
							</article> <!--End Cat Article-->	 
					    </div> <!--End Container-->
						
						<!--Footer-->		
					</div> <!--End Inner-->		
				</header> <!--End Header-->
		
		</body>
    
    {% endblock %}


</html>