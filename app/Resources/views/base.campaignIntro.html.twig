<!DOCTYPE html>
<html lang="en">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        <title>{% block title %}{% endblock %}</title>
        
        {% javascripts 
            'js/main/jquery.js'
            'js/campaign/bootstrap.min.js'
            'js/campaign/jquery.parallax-1.1.3.js'
            'js/campaign/jquery.localscroll-1.2.7-min.js'
            'js/campaign/jquery.scrollTo-1.4.2-min.js'
            'js/campaign/jquery.sticky.js'
			'js/campaign/campaign.js'
			output='js/compiled-campaign.js'
        %}
        
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
           <link href="/css/jquery-ui/base/jquery-ui-1.9.1.custom.min.css" rel="stylesheet"/>
           <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css' />
           <link href='http://fonts.googleapis.com/css?family=PT+Sans:400,700,400italic,700italic' rel='stylesheet' type='text/css' />
           <link href='http://fonts.googleapis.com/css?family=Cookie' rel='stylesheet' type='text/css' />
            {% stylesheets
                'css/campaign/bootstrap.min.css'
                'css/campaign/bootstrap-responsive.min.css'
                'css/campaign/font-awesome.min.css'
                'css/campaign/responsive.css'
				'css/campaign/hca-advertise.css'
				output='css/compiled-campaign.css'
            %}
                <link href="{{ asset_url }}" rel="stylesheet"/>
            {% endstylesheets %}
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('images/favicon.ico') }}">
        <link rel="apple-touch-icon" href="images/apple_touch_icon.png" />
        <link rel="apple-touch-icon" sizes="72x72" href="images/apple_touch_icon_72x72.png"  />
        <link rel="apple-touch-icon" sizes="114x114" href="images/apple_touch_icon_114x114.png" />
        
        <script type="text/javascript">

          var _gaq = _gaq || [];
          _gaq.push(['_setAccount', 'UA-39024795-1']);
          _gaq.push(['_trackPageview']);
        
          (function() {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
          })();
        
        </script>
        
    </head>
    <body>
    {% block body %}{% endblock %}
    
    </body>

    {{ tinymce_init() }}
</html>
        