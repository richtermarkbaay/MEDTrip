{% extends '::base.institution.html.twig' %}
{% block body %}
    <div id="main-wrap">
        <header id="header">
            <div class="container">
                <div class="row">
                    <div class="span4">
                        <a href="#"><img src="/images/institution/healthcareabroad-logo.png" alt="Health Care Abroad Logo"></a>
                    </div>
                    {% if 'institution_login' != app.request.attributes.get('_route') %}
                    <div class="navbar pull-right">
                        <ul class="nav">
                            <li>
                                <a href="{{ path('institution_login') }}"><i class="icon-lock"></i>Login</a>
                            </li>
                        </ul>
                    </div>
                    {% endif %}
                </div>
            </div>
        </header>
    <!-- Page content here -->
    {% block content %}{% endblock %}
    {% include 'InstitutionBundle:Widgets:footer.layout.html.twig' %}
</div>

    
{% block inlineJavascriptCode %}{% endblock %}
{% endblock %}