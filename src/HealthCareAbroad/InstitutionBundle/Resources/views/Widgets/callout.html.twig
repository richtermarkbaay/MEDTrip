{% if callout|length %}
    <div id="featured">
    	<div class="container">
            {% if callout['highlight'] is defined %}
        	    <h4>{{ callout['highlight'] | raw }}</h4>
            {% endif %}
        	<p>{{ callout['subText'] is defined ? callout['subText'] | raw : '' }}</p>
    	</div>
    </div>
{% endif %}