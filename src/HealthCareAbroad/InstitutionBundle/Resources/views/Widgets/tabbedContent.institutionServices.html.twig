{% embed 'InstitutionBundle:Widgets:tabbedContent.base.html.twig' %}
    {% block tabbedContent %}
        <table class="table table-striped table-bordered bootstrap-datatable">
            <thead>
                <tr>
    				<th>Services</th>
    				<th>Actions</th>
    			</tr>
            </thead>
			<tbody>
    			{% for _service in services.globalList %}
        			
        			{% include 'InstitutionBundle:Institution:row.ancillaryService.html.twig' with {'ancillaryService': _service, '_isSelected': _service.id in services.selectedAncillaryServices } %}
                    
    		    {% else %}
        		  	<tr>
        			   <td> No Services yet. </td>
        			   <td></td>
        			</tr>
    			{% endfor %}
		    </tbody>
	    </table>
    {% endblock %}
{% endembed %}