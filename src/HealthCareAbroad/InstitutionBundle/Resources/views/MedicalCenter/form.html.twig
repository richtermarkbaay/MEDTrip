<form novalidate="novalidate" action="{{ formUrl }}" method="POST" {{ form_enctype(form) }} class="basic-form">

        {{ form_row(form.medicalCenter) }}
        {{ form_row(form.description) }}
        {{ form_rest(form) }}

        {#% if hideFormButtons is not defined or not hideFormButtons %#}
        <input type="reset" value="Reset" class="form-reset"  />
        <input style="" type="submit" name="submit" value="Save" class="green-button" />
        {#% endif %#}

        <!-- end id-form  -->
</form>