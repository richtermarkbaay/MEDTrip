<tr id="award{{ award.id }}">
	<td><h3 class="award_name">{{ award.name }}</h3><small class="awarding_body_name"> {{ award.awardingBody.name }} </small></td>
	<td>2012</td>
	<td>
	    <i class="icon-edit" title="Edit date acquired"></i>
        <a class="edit_global_award" href="{{ path('institution_medicalCenter_ajaxEditGlobalAward', {'imcId':institutionMedicalCenter.id} ) }}" data-label="{{ award.name }}" data-globalAwardId="{{ award.id }}" >
            <small>Edit</small>
        </a>
		<i class="icon-trash" title="Delete"></i>
		<a data-toggle="modal" data-id="{{award.id}}" data-target="#globalAwardModal"  onclick="InstitutionMedicalCenter.showCommonModalId(this); return false;" href="#">
            <small>Delete</small></a>
	</td>
</tr>
 