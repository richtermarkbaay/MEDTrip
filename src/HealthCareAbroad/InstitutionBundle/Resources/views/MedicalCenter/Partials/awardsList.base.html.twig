<div id="{{ awardsList.containerId }}">
    {{ awardsList.selectorFieldLabel | raw }}
    {{ awardsList.selectorFieldWidget | raw }}
    
    <table class="table table-striped table-bordered {{ awardsList.tableId }} ">
        <thead>
            <tr>
				<th>{{ awardsList.header }}</th>
				<th>Actions</th>
			</tr>
        </thead>
		<tbody>
			<tr style="display:none;"  class="loader"><td colspan="2" >Loading...</td></tr>
			{% for _award in awardsList.list %}
			    {% include 'InstitutionBundle:MedicalCenter/Partials:row.globalAward.html.twig' with {'award': _award.global_award} %}
			{% endfor %}
		</tbody>
	</table>
</div>