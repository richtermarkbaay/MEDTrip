{% extends '::base.admin.html.twig' %}

{% block title %}{{ parent() }} &raquo; Provider Panel{% endblock %}

{% block body %}

<body> 
    <!-- Start: page-top-outer -->
    <div id="page-top-outer">    
    
        <!-- Start: page-top -->
        <div id="page-top">
        
            <!-- start logo -->
            <div id="logo">
                <a href=""><img src="{{ asset('images/admin/shared/logo.jpeg') }}" width="156" height="40" alt="" /></a>
            </div>
            <!-- end logo -->
            
            {% block top_search %}
                {% include 'ProviderBundle:Default:searchWidget.html.twig' %}
            {% endblock %}
            <div class="clear"></div>
        
        </div>
        <!-- End: page-top -->
    
    </div>
    <!-- End: page-top-outer -->
    
    <div class="clear">&nbsp;</div>
 
    <!--  start nav-outer-repeat................................................................................................. START -->
    <div class="nav-outer-repeat"> 
        <!--  start nav-outer -->
        <div class="nav-outer"> 
    
            {% block right_navigation %}
                {% include 'ProviderBundle:Default:rightNavigation.html.twig' %}
            {% endblock %}
            
            {% block main_navigation %}
                {% include 'ProviderBundle:Default:mainNavigation.html.twig' %}
            {% endblock %}
        </div>
        <div class="clear"></div>
        <!--  end nav-outer -->
    </div>
    <!--  end nav-outer-repeat................................................... END -->

    <!-- start content-outer ........................................................................................................................START -->
    <div id="content-outer"> 
    {% block content %}{% endblock %}
    </div>
    <!--  end content-outer........................................................END -->

    <div class="clear">&nbsp;</div>
    
    {% include '::footer.admin.html.twig' %}
 
</body>

{% endblock %}

