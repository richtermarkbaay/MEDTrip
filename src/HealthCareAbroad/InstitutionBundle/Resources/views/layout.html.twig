{% extends '::base.institution.html.twig' %}

{% block title %}{% endblock %}

{% block body %}
<div id="main-wrap">
    <!-- header -->
    {% include 'InstitutionBundle:Widgets:header.layout.html.twig' %}

    <!-- callout -->
    {% set callout = app.session.flashbag.get('callout_message') %}
    {% if callout[0] is defined %}
        {% set callout = callout[0] %}
        {% include 'InstitutionBundle:Widgets:callout.html.twig' %}
    {% endif %}

    <!-- content -->
    {% block content %}{% endblock %}
</div>
    <!-- footer -->
    {% include 'InstitutionBundle:Widgets:footer.layout.html.twig' %}

    <div id="common_modal_container" style="display:none;"></div>
    
    {% block inlineJavascriptCode %}{% endblock %}

{% endblock %}
