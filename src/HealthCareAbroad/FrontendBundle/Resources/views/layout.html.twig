{% extends '::base.frontend.html.twig' %}

{% block title %}{{ parent() }} &raquo; Healthcare Abroad{% endblock %}

{% block body %}
    <div id="fixed_container">

        {% block header %}
            {% include 'FrontendBundle:Widgets:header.html.twig' %}
        {% endblock %}
        {% include 'FrontendBundle:Widgets:breadcrumbs.html.twig' %}
        
        {% block callout %}{% endblock %}
        
        <div id="content" class="container content">
            {% block content %}{% endblock %}
        </div>
        <div class="border_pattern_footer">&nbsp;</div>

        {% block footer %}
            {% include 'FrontendBundle:Widgets:footer.html.twig' %}
        {% endblock %}

    </div>

    {{ outputInlineJavascript() | raw }}

{% endblock %}


