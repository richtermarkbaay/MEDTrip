{% extends '::base.frontend.html.twig' %}

{% block title %}{{ parent() }} &raquo; Healthcare Abroad{% endblock %}


{% block body %}
	<body>
 
	  	<div class="fixed">		
	  		{% block top_header %}
				{% include 'PageBundle:Default:TopHeader.html.twig' %}
			{% endblock %}
			{% block header %}
				{% include 'PageBundle:Default:header.html.twig' %}
			{% endblock %}
			{% block main_navigation %}
				{% include 'PageBundle:Default:navigation.html.twig' %}
			{% endblock %}
			<div class="inner">	
				<!-- News Ticker -->	
			</div> <!--End Inner-->
			
			<div class="inner">
		   		<div class="container">
		   			 {% block content %}{% endblock %}
				</div>
			</div>
			
			{% block footer %}
				{% include 'PageBundle:Footer:footer.html.twig' %}
			{% endblock %}
		
		</div>
		
		<div class="scrollTo_top">
		
		    <a title="Scroll To Top" href="#">
			<img src="{{ asset('images/frontend/up.png') }}" alt="Scroll To Top" />
		    </a>
		
		</div>
  		{{ outputInlineJavascript() | raw }}
	</body>  	
{% endblock %}


