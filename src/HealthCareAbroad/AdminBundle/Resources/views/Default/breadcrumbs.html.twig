<div class="breadcrumbs" style="max-width: 1260px; min-width: 780px; margin: 0px auto; padding: 15px 0px 0px;">
    {% for ancestor in ancestors %}
        <a href="{{ path(ancestor.node.route, routeParams) }}">{{ ancestor.node.label }}</a>
        &raquo;
    {% endfor %}
    {{ currentNode.node.label }}
</div>
