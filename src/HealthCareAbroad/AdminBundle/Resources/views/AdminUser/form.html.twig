<form novalidate="novalidate" action="{{ form_url }}" method="POST" {{ form_enctype(form) }} class="basic-form">
                        
    {{ form_row(form.firstName) }}
    {{ form_row(form.middleName) }}
    {{ form_row(form.lastName) }}
    {{ form_row(form.email) }}
    {{ form_rest(form) }}
    
    <input type="submit" name="submit" value="" class="form-submit" />
</form>