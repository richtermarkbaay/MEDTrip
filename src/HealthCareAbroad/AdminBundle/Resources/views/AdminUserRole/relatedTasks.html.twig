<!--  start related-activities -->
<div id="related-activities">
	
	<!--  start related-act-top -->
	<div id="related-act-top">
	<img src="/images/admin/forms/header_related_act.gif" width="271" height="43" alt="" />
	</div>
	<!-- end related-act-top -->
	
	<!--  start related-act-bottom -->
	<div id="related-act-bottom">
	
		<!--  start related-act-inner -->
		<div id="related-act-inner">
		    
		    <div class="left"><a href="{{ path('admin_userType_add') }}"><img src="/images/admin/forms/icon_edit.gif" width="21" height="21" alt="" /></a></div>
			<div class="right">
				<h5>Back to user types</h5>
				Create new user type
				<ul class="greyarrow">
					<li><a href="{{ path('admin_userType_index') }}">Click here to view all user types</a></li>
					{% if userType.id is defined and userType.id %}
					<li><a href="{{ path('admin_userType_edit', {'id':userType.id}) }}">Go back to {{ userType.name }} User Type</a></li>
					{% endif %}
				</ul>
			</div>
			<div class="clear"></div>
			
			<div class="lines-dotted-short"></div>
			
			<div class="left"><a href="{{ path('admin_settings') }}"><img src="/images/admin/forms/icon_edit.gif" width="21" height="21" alt="" /></a></div>
			<div class="right">
				<h5>Back to admin settings</h5>
				Go to admin settings
				<ul class="greyarrow">
					<li><a href="{{ path('admin_settings') }}">Go back to admin settings</a></li>
				</ul>
			</div>
			<div class="clear"></div>
			
		</div>
		<!-- end related-act-inner -->
		<div class="clear"></div>
	
	</div>
	<!-- end related-act-bottom -->

</div>
<!-- end related-activities -->