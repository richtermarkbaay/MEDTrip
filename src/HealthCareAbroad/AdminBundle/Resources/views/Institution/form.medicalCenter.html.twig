<div id="page-heading"><h3>Add Medical Procedure</h3></div>
{% embed '::contentWrapper.admin.html.twig' %}
	{% block wrappedContent %}
		<form action="{{ path('admin_institution_saveMedicalCenter', {id:id}) }}" method="post" {{ form_enctype(form) }} class="basic-form">		
			{{ form_row(form.medicalCenter) }}
			{{ form_row(form.description) }}
			{{ form_row(form.status) }}
			{{ form_rest(form) }}
		    <input type="submit" class="form-submit" name="submit"/>
		</form>
	{% endblock %}
{% endembed %}