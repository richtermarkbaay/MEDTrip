{% extends '::base.html.twig' %}
{% block body %}

{% if app.session.hasFlash('flash.notice') %}
    <div class="msg-notice">
        {{ app.session.flash('flash.notice') }}
    </div>
{% endif %}

<form action="{{ path('ProviderBundle_create_token') }}" method="POST" {{ form_enctype(form) }} class="user">
	{{ form_errors(form) }}
	<div class="body">
		{{ form_widget(form) }}  
        <p><input type="submit" value="Submit"></p>
        {{ form_rest(form) }}
    </div>
</form>
{% endblock %}